{
  "name": "crocotile-webgl",
  "version": "0.0.1",
  "description": "Load and render Crocotile 3D files with WebGL",
  "license": "MIT",
  "readme": "README.md",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "build": "webpack",
    "build-debug": "webpack --config webpack.config.debug.js",
